{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "Angular + Webpack build with Karma tests support",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/protractor/bin/webdriver-manager update",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "rm -rf dest && cross-env NODE_ENV=production webpack",
    "clean": "rm -rf dest coverage",
    "test-server-stop": "cross-env NODE_ENV=test BLUEBIRD_WARNINGS=0 karma stop",
    "test-server": "cross-env NODE_ENV=test BLUEBIRD_WARNINGS=0 karma start ./karma.conf.js --no-single-run",
    "test": "cross-env NODE_ENV=test BLUEBIRD_WARNINGS=0 karma run ./karma.conf.js --browsers PhantomJS",
    "e2e-server": "./node_modules/protractor/bin/webdriver-manager start",
    "validate": "eslint .",
    "mobiledebug": "weinre --boundHost -all-"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "author": {
    "name": "Nik Butenko",
    "email": "nik@butenko.me"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/packetloop/angular-webpack.git"
  },
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "babel-loader": "^5.3.2",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-webpack": "0.0.5",
    "cross-env": "^2.0.0",
    "css-loader": "^0.15.6",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "0.0.4",
    "hamlc-loader": "0.0.11",
    "html-webpack-plugin": "^1.6.0",
    "imports-loader": "^0.6.4",
    "isparta-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.3",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "merge-stream": "^0.1.8",
    "ng-cache-loader": "0.0.9",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.17",
    "protractor": "^4.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.12.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-bootstrap": "^0.12.2",
    "angular-route": "^1.4.3",
    "bootstrap": "^3.3.7",
    "jquery": "^1.12.4",
    "ui-bootstrap": "^0.1.2"
  }
}
